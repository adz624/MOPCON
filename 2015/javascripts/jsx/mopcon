var ReactCSSTransitionGroup=React.addons.CSSTransitionGroup,MopconIntro=React.createClass({displayName:"MopconIntro",getInitialState:function(){return{data:[],dataTemp:{},photoCounts:10,photoChangeTimer:1e4,currentCount:0}},changePhoto:function(){var t=this.state.data;tempPhoto=t[0],t.push(tempPhoto),this.setState({data:t}),setTimeout(function(){t.splice(0,1),this.setState({data:t})}.bind(this),600)},componentDidMount:function(){photos=[];var t="https://api.flickr.com/services/feeds/photos_public.gne?jsoncallback=?";$.getJSON(t,{tags:"mopcon",tagmode:"all",format:"json"}).done(function(t){console.log(t),perpagePhoto=t.items.length,photoCounts=t.items.length,imgSize="z";for(var e=photoCounts-1;e>=0;e--)photosNum=Math.floor(Math.random()*perpagePhoto),url=t.items[photosNum].media.m.replace("_m","_z"),photo={url:url},photos.push(photo);this.setState({dataTemp:t,data:photos})}.bind(this)),setInterval(function(){this.changePhoto()}.bind(this),this.state.photoChangeTimer)},render:function(){var t=this.state.data.map(function(t,e){return bgstyle={backgroundImage:"url("+t.url+")"},React.createElement("li",{style:bgstyle,ref:"photo_"+e,className:"list"})}.bind(this));return React.createElement("ul",{className:"list-bg"},React.createElement(ReactCSSTransitionGroup,{transitionName:"reactAnimation"},t))}});document.getElementById("mopcon")&&React.render(React.createElement(MopconIntro,null),document.getElementById("mopcon"));